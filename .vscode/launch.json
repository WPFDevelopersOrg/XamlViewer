            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/XamlViewer.exe",
            "args":[],
            "cwd": "${workspaceFolder}\\src\\XamlViewer",
            "externalConsole": true,
            "stopAtEntry": false,

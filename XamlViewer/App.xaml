<prism:PrismApplication x:Class="XamlViewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/">
    <Application.Resources>
         <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Examples.xaml"/>
                <ResourceDictionary Source="Resources/Constants.xaml"/>
                <ResourceDictionary Source="Resources/Geometries.xaml"/>
                <ResourceDictionary Source="Resources/DataTemplates/XamlSelection.xaml"/>
                <ResourceDictionary Source="Resources/DataTemplates/ReferenceSelection.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/XamlTheme;component/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
